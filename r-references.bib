@article{gomarusEffectsMemoryLoad2006,
  title = {The Effects of Memory Load and Stimulus Relevance on the {{EEG}} during a Visual Selective Memory Search Task: {{An ERP}} and {{ERD}}/{{ERS}} Study},
  shorttitle = {The Effects of Memory Load and Stimulus Relevance on the {{EEG}} during a Visual Selective Memory Search Task},
