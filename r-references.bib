@article{rhodesOrientationContingentFaceAftereffects2004,
  title = {Orientation-{{Contingent Face Aftereffects}} and {{Implications}} for {{Face-Coding Mechanisms}}},
  author = {Rhodes, Gillian and Jeffery, Linda and Watson, Tamara L. and Jaquet, Emma and Winkler, Chris and Clifford, Colin W.G.},
